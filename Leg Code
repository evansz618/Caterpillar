time.sleep_ms(500)

servos = []

for i in range(4):
    servos.append(Servo(i)) # I assume shoulder is set to the [0] servo spot and knee is for [1] servo spot

def swing():
    shoulder = shoulder_180
    knee = knee_180
    for i in range(50):
        shoulder_ang = shoulder[i]
        knee_ang = knee[i]
        servos[0].setAngle(shoulder_ang)
        servos[2].setAngle(knee_ang)
        time.sleep_ms(100)

def move_Under():
    shoulder = shoulder_0
    knee = knee_0
    for i in range(5):
        i=i+50
        shoulder_ang = shoulder[i]
        knee_ang = knee[i]
        servos[0].setAngle(shoulder_ang)
        servos[2].setAngle(knee_ang)
        time.sleep_ms(100)

def move_Back():
    shoulder = shoulder_0
    knee = knee_0
    for i in range(5):
        i=i+55
        shoulder_ang = shoulder[i]
        knee_ang = knee[i]
        servos[0].setAngle(shoulder_ang)
        servos[2].setAngle(knee_ang)
        time.sleep_ms(100)
